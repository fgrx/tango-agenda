{% extends "FabienEventsEngineBundle::layout.html.twig" %}


{% block title %}
Paramètres
{% endblock %}

{% block body %}
<div class='container'>
<h1>Paramètres</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ parameter.id }}</td>
            </tr>
            <tr>
                <th>Tokenfb</th>
                <td>{{ parameter.tokenfb }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('parameter_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('parameter_edit', { 'id': parameter.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
</div>
{% endblock %}
