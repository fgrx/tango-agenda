<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">

<channel>

	<title>Tango-agenda</title>
	<link>http://www.tango-agenda.com</link>
	<description>Tango videos</description>

	<language>fr-FR</language>


{% for video in videos %}
<item>
  <title>{{ video.title }}</title>
  <category>video</category>

  {% set ladate="" %}


  {% set ladate= video.datePublication|localizeddate('none', 'none', "","Europe/Paris","dd/MM/Y") %}

  {% if (loop.first) %}
  <guid isPermaLink="false">https://www.youtube.com//watch?v={{ video.youtubeId   }}</guid>
  <link>https://www.youtube.com//watch?v={{ video.youtubeId   }}</link>
  {% endif %}

  <description>
    <![CDATA[
    {% if video.urlImage is defined %}
      <img class="img-responsive" src='
          {{ video.urlImage }}
        ' alt='{{ video.title }}'/>
    {% endif %}
  <br />{{ video.description|striptags }}
  ]]>
</description>
</item>

{% endfor %}





</channel>
</rss>
