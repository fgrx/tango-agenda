<?xml version="1.0" encoding="UTF-8"?>
<urlset
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9 http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd
    http://www.w3.org/1999/xhtml http://www.w3.org/2002/08/xhtml/xhtml1-strict.xsd"
    xmlns="http://www.sitemaps.org/schemas/sitemap/0.9"
    xmlns:xhtml="http://www.w3.org/1999/xhtml"
>

{% for element in elements %}
<url>
  <loc>http://www.tango-agenda.com/fr/{{element.url}}</loc>
  <changefreq>{{element.changefreq}}</changefreq>
  <priority>{{element.priority}}</priority>
  <xhtml:link rel="alternate" hreflang="en" href='http://www.tango-agenda.com/en/{{element.url}}' />
 </url>
{% endfor %}



</urlset>
